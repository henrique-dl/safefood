{
  "expo": {
    "name": "SafeFood",
    "slug": "SafeFood",
    "scheme": "safefood",
    "version": "0.0.10",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
